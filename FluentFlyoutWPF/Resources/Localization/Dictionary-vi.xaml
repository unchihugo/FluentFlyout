    <system:String x:Key="EasingCubic" xml:space="preserve"> Hàm số bậc ba</system:String>
    <system:String x:Key="NativeExperienceInfo">Để có trải nghiệm thuần Windows 11, các cài đặt nên được giữ nguyên.</system:String>
    <system:String x:Key="DisableOnFullscreenTitle">Vô hiệu hóa flyout trong các cửa sổ DirectX toàn màn hình nếu phát hiện</system:String>
