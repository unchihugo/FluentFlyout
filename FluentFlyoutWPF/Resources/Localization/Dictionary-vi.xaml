<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="TitleText">Cài đặt FluentFlyout</system:String>
    <system:String x:Key="MediaFlyoutTitle">Tinh chỉnh Flyout Media</system:String>
    <system:String x:Key="MediaFlyoutDescription">Khi đang phát nhạc hoặc các âm thanh khác, bạn có thể làm hiển thị flyout media bằng cách nhấn một phím điều khiển media (phát/dùng , ...) hoặc tăng/giảm âm lượng. Flyout media hiển thị thông tin nhạc, cùng với các phím điều khiển và nhiều hơn thế nữa.</system:String>
    <system:String x:Key="TextBlockText1">Bật flyout media</system:String>
    <system:String x:Key="TextBlockText3">Hiện flyout khi bạn nhấn phím media</system:String>
    <system:String x:Key="CompactLayoutTitle">Giao diện gọn gàng</system:String>
    <system:String x:Key="CompactLayoutDescription" xml:space="preserve">Giao diện nhỏ gọn hơn và giảm bớt bố cục rắc rối
ẩn tên chương trình phát âm thanh, nút 'lặp lại' và 'trộn bài'</system:String>
    <system:String x:Key="FlyoutPositionTitle">Vị trí flyout</system:String>
    <system:String x:Key="PositionBottomLeft" xml:space="preserve"> Góc trái bên dưới</system:String>
    <system:String x:Key="PositionBottomCenter" xml:space="preserve"> Ở giữa phía dưới</system:String>
    <system:String x:Key="PositionBottomRight" xml:space="preserve"> Góc phải bên dưới</system:String>
    <system:String x:Key="PositionTopLeft" xml:space="preserve"> Góc trái bên trên</system:String>
    <system:String x:Key="PositionTopCenter" xml:space="preserve"> Ở giữa phía trên</system:String>
    <system:String x:Key="PositionTopRight" xml:space="preserve"> Góc phải bên trên</system:String>
    <system:String x:Key="FlyoutStayDurationTitle">Thời gian hiển thị</system:String>
    <system:String x:Key="FlyoutStayDurationDescription">(mặc định: 3000 mili giây)</system:String>
    <system:String x:Key="MillisecondsUnit">mili giây</system:String>
    <system:String x:Key="CenterTitleArtistTitle">Căn giữa tiêu đề và tên nhạc sĩ</system:String>
    <system:String x:Key="CenterTitleArtistDescription">Căn giữa văn bản thay vì căn lề trái</system:String>
    <system:String x:Key="ShowMediaPlayerNameTitle">Hiển thị tên phần mềm phát nhạc</system:String>
    <system:String x:Key="RepeatButtonTitle">Nút lặp lại*</system:String>
    <system:String x:Key="RepeatButtonDescription">Chọn chế độ lặp lại liên tục hay lặp lại một lần, tăng chiều rộng của flyout</system:String>
    <system:String x:Key="ShuffleButtonTitle">Nút trộn bài*</system:String>
    <system:String x:Key="ShuffleButtonDescription">Tăng chiều rộng của flyout</system:String>
    <system:String x:Key="ShowSeekbarTitle">Hiển thị thanh thời gian</system:String>
    <system:String x:Key="ShowSeekbarDescription">Cho phép tua đi tua lại media hiện tại</system:String>
    <system:String x:Key="UnsupportedControlsWarning">*Một số trình duyệt/phần mềm phát nhạc không hỗ trợ lặp lại, trộn bài hoặc thanh thời gian.</system:String>
    <system:String x:Key="PauseOtherMediaTitle">Tự động tạm dừng media khác</system:String>
    <system:String x:Key="PauseOtherMediaDescription">Khi bạn phát hoặc tập trung vào một bản nhạc, các media khác sẽ tự động tạm dừng</system:String>
    <system:String x:Key="NextUpCustomizationTitle">Tinh chỉnh bài hát tiếp theo</system:String>
    <system:String x:Key="NextUpDescription" xml:space="preserve">Flyout hiển thị những gì tiếp theo khi một bài hát/video kết thúc. Chỉ hiển thị nếu thứ tiếp đến tự động phát, nếu phím đi tiếp/tua lại được nhấn thủ công, flyout chính sẽ hiện lên thay thế.</system:String>
    <system:String x:Key="EnableNextUpTitle">Bật flyout dự báo bài hát tiếp theo (Thử nghiệm)</system:String>
    <system:String x:Key="EnableNextUpDescription">Hiển thị những gì sẽ tới khi một bài hát/video kết thúc</system:String>
    <system:String x:Key="NextUpStayDurationTitle">Thời gian hiển thị bài hát tiếp theo</system:String>
    <system:String x:Key="NextUpStayDurationDescription">(mặc định: 2000 mili giây)</system:String>
    <system:String x:Key="LockKeysCustomizationTitle">Tinh chỉnh phím khóa</system:String>
    <system:String x:Key="LockKeysDescription" xml:space="preserve">Kiểm tra phím khóa của bạn tức thì. Hiển thị sau khi nhấn phím Caps Lock, Num Lock hay Scroll Lock. Tiện lợi cho các laptop/các bàn phím không có đèn báo hiệu khóa phím.</system:String>
    <system:String x:Key="EnableLockKeysTitle">Bật flyout phím khóa</system:String>
    <system:String x:Key="EnableLockKeysDescription">Cho biết phím chuyển đổi đang bật hoặc đang tắt</system:String>
    <system:String x:Key="LockKeysStayDurationTitle">Thời gian chờ hiển thị phím khóa</system:String>
    <system:String x:Key="LockKeysStayDurationDescription">(mặc định: 2000 mili giây)</system:String>
    <system:String x:Key="LockKeysBoldUITitle">Phông chữ và biểu tượng đậm</system:String>
    <system:String x:Key="LockKeysBoldUIDescription">Tô đậm phông chữ và biểu tượng của flyout</system:String>
    <system:String x:Key="EnableInsertKeyTitle">Bật hiển thị khi nhấn phím Insert</system:String>
    <system:String x:Key="EnableInsertKeyDescription">Cho biết phím Insert đang bật hoặc đang tắt</system:String>
    <system:String x:Key="SystemSettingsTitle">Hệ thống</system:String>
    <system:String x:Key="LaunchOnStartupTitle">Khởi động trong khi bật máy</system:String>
    <system:String x:Key="LaunchOnStartupDescription">Khởi động ẩn trong thanh công cụ khi bạn đăng nhập vào Winddows</system:String>
    <system:String x:Key="StartupAppsSettingsInfo">Nếu cài đặt này không hoạt động như mong đợi, kiểm tra</system:String>
    <system:String x:Key="AppThemeTitle">Giao diện ứng dụng</system:String>
    <system:String x:Key="AppThemeDefault"> Mặc định khớp với Windows</system:String>
    <system:String x:Key="AppThemeLight"> Sáng</system:String>
    <system:String x:Key="AppThemeDark"> Tối</system:String>
    <system:String x:Key="TrayIconLeftClickBehaviorTitle">Cách hiển thị khi nhấn chuột trái vào biểu tượng ứng dụng trong khay hệ thống</system:String>
    <system:String x:Key="TrayIconLeftClickBehaviorDescription">Chọn những gì sẽ hiện ra khi bạn nhấn chuột trái vào biểu tượng trong khay hệ thống</system:String>
    <system:String x:Key="TrayIconLeftClickOption1"> Mở cài đặt</system:String>
    <system:String x:Key="TrayIconLeftClickOption2"> Hiển thị flyout media</system:String>
    <system:String x:Key="Win11TrayIconTitle">Biểu tượng ứng dụng trong khay khớp với Windows 11</system:String>
    <system:String x:Key="Win11TrayIconDescription">Sử dụng biểu trưng đơn giản hơn trong system tray</system:String>
    <system:String x:Key="AnimationSettingsTitle">Cài đặt hiệu ứng chuyển động</system:String>
    <system:String x:Key="AnimationSettingsDescription">Tinh chỉnh hiệu ứng chuyển động theo cách bạn thích</system:String>
    <system:String x:Key="AnimationDurationScaleTitle">Tốc độ hiệu ứng chuyển động</system:String>
    <system:String x:Key="AnimationEasingStyleTitle">Hiệu ứng chuyển động mượt mà</system:String>
    <system:String x:Key="AnimationSpeedOff">Tắt chuyển động</system:String>
    <system:String x:Key="AnimationDurationScaleDescription">Tốc độ chuyển động nhanh hơn hoặc chậm hơn (mặc định: 1x)</system:String>
    <system:String x:Key="AnimationEasingStyleDescription">(mặc định: Nhanh)</system:String>
    <system:String x:Key="EasingLinear" xml:space="preserve"> Bình thường</system:String>
    <system:String x:Key="EasingSine" xml:space="preserve"> Mượt</system:String>
    <system:String x:Key="EasingQuadratic" xml:space="preserve"> Nhanh</system:String>
    <system:String x:Key="EasingCubic" xml:space="preserve"> Nhanh nhất</system:String>
    <system:String x:Key="NativeExperienceInfo">Để có trải nghiệm thuần Windows 11, các cài đặt nên được giữ nguyên.</system:String>
    <system:String x:Key="DisableOnFullscreenTitle">Vô hiệu hóa flyout trong các cửa sổ DirectX toàn màn hình nếu phát hiện</system:String>
    <system:String x:Key="DisableOnFullscreenDescription">Tránh các trò chơi sử dụng DirectX trong chế độ toàn màn hình khi thu nhỏ cửa sổ nếu flyout hiện lên</system:String>
    <system:String x:Key="AppLanguageTitle">Ngôn ngữ ứng dụng</system:String>
    <system:String x:Key="EnjoyingAppText" xml:space="preserve">Nếu bạn cảm thấy chương trình này hữu ích, bạn có thể gắn sao repo GitHub
hoặc viết một bài đánh giá trên Microsoft Store để ủng hộ tác giả!</system:String>
    <system:String x:Key="GitHubRepoLink">Repo GitHub</system:String>
    <system:String x:Key="CloseButton">Nút đóng</system:String>
    <system:String x:Key="AcrylicWindowTitle">Cửa sổ Acrylic</system:String>
    <system:String x:Key="AcrylicWindowDescription">Áp dụng hiệu ứng acrylic mờ trong suốt cho flyout (khi tắt thì quay về sử dụng Mica)</system:String>
    <system:String x:Key="BackgroundBlurTitle">Mờ nền</system:String>
    <system:String x:Key="BackgroundBlurOption1" xml:space="preserve"> Không làm mờ</system:String>
    <system:String x:Key="BackgroundBlurOption2" xml:space="preserve"> Kiểu 1 (Ảnh bìa phát sáng)</system:String>
    <system:String x:Key="BackgroundBlurOption3" xml:space="preserve"> Kiểu 2 (Hào quang lan tỏa)</system:String>
    <system:String x:Key="BackgroundBlurOption4" xml:space="preserve"> Kiểu 3 (Mờ)</system:String>
    <system:String x:Key="TextBlockText2">Lựa chọn từ các bản mẫu hiệu ứng làm mờ nền hiện đại</system:String>
    <system:String x:Key="StartupAppsHyperlinkText">Các ứng dụng khởi động cùng hệ thống</system:String>
    <system:String x:Key="AppLanguageDescription" xml:space="preserve">Nếu ngôn ngữ của bạn không có trong danh sách hoặc chưa hoàn thiện, nó sẽ mặc định là tiếng Anh.
Bạn có thể giúp chúng tôi bằng việc đóng góp các bản dịch trên</system:String>
    <system:String x:Key="AppLanguageDescriptionLink">Liên kết đóng góp bản dịch</system:String>
    <system:String x:Key="MediaFlyoutAlwaysDisplay">Màn hình luôn bật</system:String>
