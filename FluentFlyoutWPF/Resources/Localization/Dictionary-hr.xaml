<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="TitleText">FluentFlyout Postavke</system:String>
    <system:String x:Key="MediaFlyoutTitle">Uređivanje iskočnog prozora za medije</system:String>
    <system:String x:Key="MediaFlyoutDescription">Glavni iskočni prozor. Prilikom slušanja medija, pritisnite bilo koju tipku za medije ili glasnoću (pokreni/zaustavi, pojačavanje/smanjivanje glasnoće, itd.) na tipkovnici da se prikaže iskočni prozor. Prikazuje informacije o mediju, kontrole reprodukcije i više.</system:String>
    <system:String x:Key="TextBlockText1">Omogući iskočni prozor za medije</system:String>
    <system:String x:Key="TextBlockText3">Prikaži iskočni prozor pritiskom tipki za medije</system:String>
    <system:String x:Key="TextBlockText2">Odaberite iz unaprijed postavljenih modernih stilova zamućenja pozadine</system:String>
    <system:String x:Key="BackgroundBlurOption2" xml:space="preserve"> Stil 1 (Pozadinski Sjaj)</system:String>
    <system:String x:Key="BackgroundBlurOption3" xml:space="preserve"> Stil 2 (Izlazeći Sjaj)</system:String>
    <system:String x:Key="BackgroundBlurOption4" xml:space="preserve"> Stil 3 (Zamućenje)</system:String>
    <system:String x:Key="AcrylicWindowTitle">Akrilni prozor</system:String>
    <system:String x:Key="AcrylicWindowDescription">Primijeni prozirni akrilni zamućeni efekt na iskočni prozor (isključenje vraća na "Mica" efekt)</system:String>
    <system:String x:Key="CompactLayoutDescription" xml:space="preserve">Tanji i manje nametljiv raspored skriva ponavljanje,
nasumičnu reprodukciju i informacije o mediju</system:String>
    <system:String x:Key="SelectedMonitorTitle">Zaslon za prikaz iskočnog prozora</system:String>
    <system:String x:Key="FlyoutPositionTitle">Pozicija iskočnog prozora</system:String>
    <system:String x:Key="FlyoutStayDurationTitle">Trajanje prikaza iskočnog prozora</system:String>
    <system:String x:Key="FlyoutStayDurationDescription">(zadano: 3000 ms)</system:String>
    <system:String x:Key="MillisecondsUnit">ms</system:String>
    <system:String x:Key="CenterTitleArtistTitle">Centriraj naslov i izvođača</system:String>
    <system:String x:Key="CenterTitleArtistDescription">Poravnaj tekst u sredinu umjesto ulijevo</system:String>
    <system:String x:Key="ShowMediaPlayerNameTitle">Prikaži naziv aplikacije medija</system:String>
