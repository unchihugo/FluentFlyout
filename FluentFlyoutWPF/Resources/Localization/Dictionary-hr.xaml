    <system:String x:Key="ShuffleButtonTitle">Gumb za nasumično reproduciranje*</system:String>
    <system:String x:Key="ShuffleButtonDescription">Povećava širinu iskočnog prozora</system:String>
    <system:String x:Key="ShowSeekbarTitle">Prikaži traku za traženje*</system:String>
    <system:String x:Key="ShowSeekbarDescription">Interaktivna traka za traženje će se automatski prikazati ako ju aplikacija podržava</system:String>
    <system:String x:Key="UnsupportedControlsWarning">*Neki preglednici/aplikacije ne podržavaju ponavljanje, nasumičnu reprodukciju i traku za traženje.</system:String>
    <system:String x:Key="PauseOtherMediaTitle">Pauziraj druge medije automatski</system:String>
