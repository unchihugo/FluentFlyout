    <system:String x:Key="TaskbarWidgetEnabledTitle">Увімкнути віджет панелі завдань</system:String>
    <system:String x:Key="TaskbarWidgetEnabledDescription">Показати компактний медіа-віджет на панелі завдань</system:String>
