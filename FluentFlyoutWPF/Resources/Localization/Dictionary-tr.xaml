    <system:String x:Key="EnableInsertKeyTitle">Insert Tuşu Açılır Penceresini Etkinleştir</system:String>
    <system:String x:Key="EnableInsertKeyDescription">Insert/overwrite düğmesine basıldığında açılır pencerenin gösterilip gösterilmeyeceğini belirler</system:String>
