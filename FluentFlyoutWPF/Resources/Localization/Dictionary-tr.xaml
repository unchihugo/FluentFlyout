    <system:String x:Key="AcrylicWindowTitle">Akrilik Pencere</system:String>
    <system:String x:Key="AcrylicWindowDescription">Açılır pencere'ye şeffaf akrilik bulanıklık efekti uygulayın (kapatıldığında Mica'ya geri döner)</system:String>
    <system:String x:Key="CompactLayoutTitle">Kompakt Düzen</system:String>
