<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib">
    
    <System:String x:Key="TitleText">FluentFlyout Settings</System:String>
    <System:String x:Key="MediaFlyoutTitle">Media Flyout Customization</System:String>
    <System:String x:Key="MediaFlyoutDescription">Main flyout. When listening to media, press any media or volume key (play/pause, volume up/down, etc.) on your keyboard to display the flyout. Displays media info, along with playback controls and more.</System:String>
    <System:String x:Key="TextBlockText1">Enable Media Flyout</System:String>
    <System:String x:Key="TextBlockText3">Display a flyout when you press a media key</System:String>
    <System:String x:Key="TextBlockText2">Choose from a preset of modern background blur styles</System:String>
    <System:String x:Key="BackgroundBlurOption1" xml:space="preserve"> None</System:String>
    <System:String x:Key="BackgroundBlurOption2" xml:space="preserve"> Style 1 (Cover Glow)</System:String>
    <System:String x:Key="BackgroundBlurOption3" xml:space="preserve"> Style 2 (Rising Glow)</System:String>
    <System:String x:Key="BackgroundBlurOption4" xml:space="preserve"> Style 3 (Blur)</System:String>
    <System:String x:Key="BackgroundBlurTitle">Background Blur</System:String>
    <System:String x:Key="AcrylicWindowTitle">Acrylic Window</System:String>
    <System:String x:Key="AcrylicWindowDescription">Apply transparent acrylic blur effect to the flyout (off reverts to Mica)</System:String>
    <System:String x:Key="CompactLayoutTitle">Compact Layout</System:String>
    <System:String x:Key="CompactLayoutDescription" xml:space="preserve">Slimmer and less intrusive layout&#10;hides repeat, shuffle and player info</System:String>
    <System:String x:Key="FlyoutPositionTitle">Flyout Position</System:String>
    <System:String x:Key="PositionBottomLeft" xml:space="preserve"> Bottom Left</System:String>
    <System:String x:Key="PositionBottomCenter" xml:space="preserve"> Bottom Center</System:String>
    <System:String x:Key="PositionBottomRight" xml:space="preserve"> Bottom Right</System:String>
    <System:String x:Key="PositionTopLeft" xml:space="preserve"> Top Left</System:String>
    <System:String x:Key="PositionTopCenter" xml:space="preserve"> Top Center</System:String>
    <System:String x:Key="PositionTopRight" xml:space="preserve"> Top Right</System:String>
    <System:String x:Key="FlyoutStayDurationTitle">Flyout Stay Duration</System:String>
    <System:String x:Key="FlyoutStayDurationDescription">(default: 3000 ms)</System:String>
    <System:String x:Key="MillisecondsUnit">ms</System:String>
    <System:String x:Key="MediaFlyoutAlwaysDisplay">Always Display</System:String>
    <System:String x:Key="CenterTitleArtistTitle">Center Title and Artist</System:String>
    <System:String x:Key="CenterTitleArtistDescription">Align text to the center instead of the left</System:String>
    <System:String x:Key="ShowMediaPlayerNameTitle">Show Media Player name</System:String>
    <System:String x:Key="RepeatButtonTitle">Repeat Button*</System:String>
    <System:String x:Key="RepeatButtonDescription">Choose between repeat all or repeat one, increases flyout width</System:String>
    <System:String x:Key="ShuffleButtonTitle">Shuffle Button*</System:String>
    <System:String x:Key="ShuffleButtonDescription">Increases flyout width</System:String>
    <System:String x:Key="ShowSeekbarTitle">Show Seekbar</System:String>
    <System:String x:Key="ShowSeekbarDescription">An interactable seekbar will automatically display if the player supports it</System:String>
    <System:String x:Key="UnsupportedControlsWarning">*Some browsers/players do not support repeat, shuffle and seekbar control.</System:String>
    <System:String x:Key="PauseOtherMediaTitle">Pause Other Media Automatically</System:String>
    <System:String x:Key="PauseOtherMediaDescription">When you play or focus on new media, other active sessions are automatically paused</System:String>
    <System:String x:Key="NextUpCustomizationTitle">Next Up Customization</System:String>
    <System:String x:Key="NextUpDescription" xml:space="preserve">A flyout shows what's next when a song/video ends. Only appears if the next item plays automatically, if next/previous is pressed manually, the main flyout appears instead.</System:String>
    <System:String x:Key="EnableNextUpTitle">Enable Next Up Flyout (Experimental)</System:String>
    <System:String x:Key="EnableNextUpDescription">Shows what's next when a song/video ends</System:String>
    <System:String x:Key="NextUpStayDurationTitle">Next Up Stay Duration</System:String>
    <System:String x:Key="NextUpStayDurationDescription">(default: 2000 ms)</System:String>
    <System:String x:Key="LockKeysCustomizationTitle">Lock Keys Customization</System:String>
    <System:String x:Key="LockKeysDescription" xml:space="preserve">Check your lock key status at a glance. Appears after pressing Caps Lock, Num Lock or Scroll Lock. Convenient for laptops/keyboards without lock key indicators.</System:String>
    <System:String x:Key="EnableLockKeysTitle">Enable Lock Keys Flyout</System:String>
    <System:String x:Key="EnableLockKeysDescription">Displays whether the specified lock key is on or off</System:String>
    <System:String x:Key="LockKeysStayDurationTitle">Lock Keys Stay Duration</System:String>
    <System:String x:Key="LockKeysStayDurationDescription">(default: 2000 ms)</System:String>
    <System:String x:Key="LockKeysBoldUITitle">Bold Symbol and Font</System:String>
    <System:String x:Key="LockKeysBoldUIDescription">Emphasizes the flyout symbol and font</System:String>
    <System:String x:Key="EnableInsertKeyTitle">Enable Insert Key Popup</System:String>
    <System:String x:Key="EnableInsertKeyDescription">Determines whether pressing insert/overwrite will show the flyout</System:String>
    <System:String x:Key="SystemSettingsTitle">System</System:String>
    <System:String x:Key="LaunchOnStartupTitle">Launch on Startup</System:String>
    <System:String x:Key="LaunchOnStartupDescription">Start minimized to tray when you log in to Windows</System:String>
    <System:String x:Key="StartupAppsHyperlinkText">Startup Apps</System:String>
    <System:String x:Key="StartupAppsSettingsInfo">If this setting does not work as expected, check</System:String>
    <System:String x:Key="AppThemeTitle">App Theme</System:String>
    <System:String x:Key="AppThemeDefault"> Windows Default</System:String>
    <System:String x:Key="AppThemeLight"> Light</System:String>
    <System:String x:Key="AppThemeDark"> Dark</System:String>
    <System:String x:Key="TrayIconLeftClickBehaviorTitle">Tray Icon Left Click Behavior</System:String>
    <System:String x:Key="TrayIconLeftClickBehaviorDescription">Choose what happens when you left click the tray icon</System:String>
    <System:String x:Key="TrayIconLeftClickOption1"> Open Settings</System:String>
    <System:String x:Key="TrayIconLeftClickOption2"> Show Media Flyout</System:String>
    <System:String x:Key="Win11TrayIconTitle">Windows 11-like Tray Icon</System:String>
    <System:String x:Key="Win11TrayIconDescription">Use a tray icon that matches Windows 11 (toggle to refresh theme)</System:String>
    <System:String x:Key="AnimationSettingsTitle">Animation Settings</System:String>
    <System:String x:Key="AnimationSettingsDescription">Tweak the animations to your liking</System:String>
    <System:String x:Key="AnimationDurationScaleTitle">Animation Duration Scale</System:String>
    <System:String x:Key="AnimationDurationScaleDescription">Faster or slower flyout animation speed (default: 1x)</System:String>
    <System:String x:Key="AnimationSpeedOff">Off</System:String>
    <System:String x:Key="AnimationEasingStyleTitle">Animation Easing Style</System:String>
    <System:String x:Key="AnimationEasingStyleDescription">(default: Quadratic)</System:String>
    <System:String x:Key="EasingLinear" xml:space="preserve"> Linear</System:String>
    <System:String x:Key="EasingSine" xml:space="preserve"> Sine</System:String>
    <System:String x:Key="EasingQuadratic" xml:space="preserve"> Quadratic</System:String>
    <System:String x:Key="EasingCubic" xml:space="preserve"> Cubic</System:String>
    <System:String x:Key="NativeExperienceInfo">For the native Windows 11 experience, the settings should be kept at default.</System:String>
    <System:String x:Key="DisableOnFullscreenTitle">Disable Flyouts if DirectX fullscreen program is detected</System:String>
    <System:String x:Key="DisableOnFullscreenDescription">Prevents DirectX games in exclusive fullscreen from minimizing if a flyout pops up</System:String>
    <System:String x:Key="EnjoyingAppText" xml:space="preserve">Enjoying the app? Star it on GitHub,&#10;or leave a review on Microsoft Store!</System:String>
    <System:String x:Key="GitHubRepoLink">GitHub Repository</System:String>
    <System:String x:Key="CloseButton">Close</System:String>
    <System:String x:Key="AppLanguageTitle">App Language</System:String>
    <System:String x:Key="AppLanguageDescription" xml:space="preserve">If your language is not listed or incomplete, it will default to English.&#10;You can help contribute translations on</System:String>
    <System:String x:Key="AppLanguageDescriptionLink">Weblate</System:String>
    <System:String x:Key="LockWindow_CapsLock">Caps Lock</System:String>
    <System:String x:Key="LockWindow_NumLock">Num Lock</System:String>
    <System:String x:Key="LockWindow_ScrollLock">Scroll Lock</System:String>
    <System:String x:Key="LockWindow_InsertPressed">Insert Pressed</System:String>
    <System:String x:Key="LockWindow_LockOn">on</System:String>
    <System:String x:Key="LockWindow_LockOff">off</System:String>
    <System:String x:Key="NextUpWindow_UpNextText">Up next:</System:String>
</ResourceDictionary>