<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:system="clr-namespace:System;assembly=mscorlib">
    <system:String x:Key="TitleText">Nastavení FluentFlyout</system:String>
    <system:String x:Key="MediaFlyoutTitle">Přizpůsobení plovoucího přehrávače</system:String>
    <system:String x:Key="MediaFlyoutDescription">Hlavní plovoucí okno. Při poslechu médií stiskněte libovolnou klávesu pro ovládání médií nebo hlasitosti (přehrávání/pozastavení, zvýšení/snížení hlasitosti atd.) na klávesnici, aby se zobrazilo plovoucí okno. Zobrazuje informace o médiích spolu s ovládacími prvky přehrávání a dalšími funkcemi.</system:String>
    <system:String x:Key="TextBlockText1">Povolit plovoucí přehrávač</system:String>
    <system:String x:Key="TextBlockText3">Zobrazí plovoucí okno při zmáčknutí klávesy pro ovládání médií</system:String>
    <system:String x:Key="TextBlockText2">Vyberte si z moderních stylů rozostření pozadí</system:String>
    <system:String x:Key="BackgroundBlurOption1" xml:space="preserve"> Žádné</system:String>
    <system:String x:Key="BackgroundBlurOption2" xml:space="preserve"> Styl 1 (Cover Glow)</system:String>
    <system:String x:Key="BackgroundBlurOption3" xml:space="preserve"> Styl 2 (Rising Glow)</system:String>
    <system:String x:Key="BackgroundBlurOption4" xml:space="preserve"> Styl 3 (Blur)</system:String>
    <system:String x:Key="BackgroundBlurTitle">Rozmazání pozadí</system:String>
    <system:String x:Key="AcrylicWindowTitle">Akrylový efekt</system:String>
    <system:String x:Key="AcrylicWindowDescription">Aplikuje průhledný akrylový efekt rozmazání na plovoucí okno (při vypnutém použije Mica)</system:String>
    <system:String x:Key="CompactLayoutTitle">Kompaktní rozvržení</system:String>
    <system:String x:Key="CompactLayoutDescription" xml:space="preserve">Užší a méně rušivé rozvržení
skryje opakování,náhodné přehrávání a informace přehrávače</system:String>
    <system:String x:Key="FlyoutPositionTitle">Pozice plovoucího okna</system:String>
    <system:String x:Key="PositionBottomLeft" xml:space="preserve"> Dole vlevo</system:String>
    <system:String x:Key="PositionBottomCenter" xml:space="preserve"> Dole uprostřed</system:String>
    <system:String x:Key="PositionBottomRight" xml:space="preserve"> Dole vpravo</system:String>
    <system:String x:Key="PositionTopLeft" xml:space="preserve"> Nahoře vlevo</system:String>
    <system:String x:Key="PositionTopCenter" xml:space="preserve"> Nahoře uprostřed</system:String>
    <system:String x:Key="PositionTopRight" xml:space="preserve"> Nahoře vpravo</system:String>
    <system:String x:Key="FlyoutStayDurationTitle">Zmizení plovoucího okna</system:String>
    <system:String x:Key="FlyoutStayDurationDescription">(výchozí: 3000ms)</system:String>
    <system:String x:Key="MillisecondsUnit">ms</system:String>
    <system:String x:Key="MediaFlyoutAlwaysDisplay">Vždy zobrazovat</system:String>
    <system:String x:Key="CenterTitleArtistTitle">Vycentrovat název písničky a autora</system:String>
    <system:String x:Key="CenterTitleArtistDescription">Vycentruje text doprostřed místo doleva</system:String>
    <system:String x:Key="ShowMediaPlayerNameTitle">Ukazovat název přehrávače</system:String>
    <system:String x:Key="EasingLinear" xml:space="preserve"> Lineární</system:String>
    <system:String x:Key="EasingSine" xml:space="preserve"> Sinus</system:String>
</ResourceDictionary>
